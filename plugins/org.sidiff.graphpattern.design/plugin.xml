<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
  <extension point="org.eclipse.sirius.componentization">
    <component class="org.sidiff.graphpattern.design.Activator"
               id="org.sidiff.graphpattern.design"
	       name="graphpattern">
    </component>
  </extension>
  <extension
        point="org.eclipse.sirius.diagram.ui.styleConfigurationProvider">
     <styleConfigurationProvider
           providerClass="org.sidiff.graphpattern.design.provider.LabelIconProvider">
     </styleConfigurationProvider>
  </extension>
  <extension
        point="org.eclipse.sirius.diagram.ui.customLayoutAlgorithmProvider">
     <customLayoutProvider
           class="org.sidiff.graphpattern.design.layout.GraphPatternAlgorithmProvider">
     </customLayoutProvider>
  </extension>
</plugin>
